{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/rxdb",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "outputPath": "dist/packages/rxdb",
        "packageJson": "packages/rxdb/package.json",
        "tsConfig": "packages/rxdb/tsconfig.json",
        "main": "packages/rxdb/pouchdb-md5.js",
        "assets": ["packages/rxdb/*.md", "packages/rxdb/*.js", "LICENSE"],
        "dependsOn": [
          {
            "target": "build.all",
            "projects": "dependencies"
          }
        ]
      }
    },
    "focus": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx g @nativescript/plugin-tools:focus-packages rxdb"],
        "parallel": false
      }
    }
  },
  "tags": []
}
